{
  "name": "swagit",
  "description": "A swag tool to use git with interactive cli",
  "license": "MIT",
  "author": "Evan Ye <https://jigsawye.com> (jigsaw.ye@gmail.com)",
  "repository": {
    "url": "https://github.com/jigsawye/swagit.git"
  },
  "bugs": {
    "url": "https://github.com/jigsawye/swagit/issues"
  },
  "version": "0.1.4",
  "bin": {
    "sg": "./bin/swagit.js",
    "swagit": "./bin/swagit.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint bin lib",
    "test": "yarn lint"
  },
  "dependencies": {
    "args": "^5.0.0",
    "chalk": "^2.4.1",
    "inquirer": "^6.0.0",
    "node-version": "^1.1.3",
    "simple-git": "^1.95.1",
    "update-check": "^1.5.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-yoctol-base": "^0.15.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5",
    "prettier-package-json": "^1.6.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "prettier --write",
      "git add"
    ],
    ".babelrc": [
      "prettier --parser json --write",
      "git add"
    ]
  }
}
